

.icon-position {
  position: absolute;
  right: 40rpx;
  bottom: 12rpx;
}

.search-wrapper {
  padding: 10rpx 30rpx;
  position: relative;
}

.search-wrapper .search {
  border-radius: 5px;
  border: 1rpx solid #eee;
  font-size: 12px;
}

.placeholder {
  font-size: 12px;
  text-align: center;
}

.search-wrapper .icon-search {
  position: absolute;
  right: 40rpx;
}

.slide-image {
  width: 90%;
  height: 90%;
  position: absolute;
  left:calc(50% - 169px);
  top:calc(50% - 67.5px);
  border-radius: 30rpx;
}
